{
  "name": "@strdcoders/radarr-api",
  "version": "1.0.0",
  "description": "A Typescript api for Radarr PVR service",
  "author": "STRDcoders",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/STRDCoders/radarr-api.git"
  },
  "scripts": {
    "build": "tsc -sourcemap -p ./tsconfig.json",
    "run": "node -r source-map-support/register dist/src/main.js",
    "start": "npm run build && npm run run",
    "test": "nyc mocha -r ts-node/register test/**/*.spec.ts --exit",
    "lint": "eslint \"./{src,test}/**/*.ts\"",
    "depcheck": "depcheck --ignores \"source-map-support,ts-node,@types/chai-http,prettier,pretty-quick,eslint-plugin-jsdoc\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.27.2",
    "depcheck": "^1.4.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "express": "^4.17.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "husky": "^4.3.0",
    "mocha": "^10.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "swagger-jsdoc": "^5.0.1",
    "typescript": "^4.1.5"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
